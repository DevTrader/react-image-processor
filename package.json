{
  "name": "react-image-processor",
  "version": "0.2.0",
  "description": "React Components built-in Image Processor.",
  "main": "lib/image_processor",
  "scripts": {
    "lint": "eslint src/**/*.{js,jsx} spec/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx} spec/**/*.{js,jsx}",
    "build": "babel src --out-dir lib --source-maps",
    "test": "mocha --opts spec/mocha.opts spec/**/*.spec.jsx",
    "story:start": "start-storybook -p 9001 -c .storybook",
    "story:build": "build-storybook -c .storybook -o doc",
    "story:deploy": "sh bin/deploy",
    "prepublish": "yarn build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenju/react-image-processor.git"
  },
  "keywords": [
    "react-components",
    "image-processing"
  ],
  "author": "kenju",
  "license": "apache-2.0",
  "bugs": {
    "url": "https://github.com/kenju/react-image-processor/issues"
  },
  "homepage": "https://github.com/kenju/react-image-processor#readme",
  "dependencies": {},
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "peerDependencies": {
    "react": "^15.4.1"
  }
}
